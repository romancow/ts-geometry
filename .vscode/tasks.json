	"version": "2.0.0",
	"tasks": [
		{
			"label": "Test",
			"type": "npm",
			"script": "test",
			"group": {
				"kind": "test",
				"isDefault": true
			}
		},
		{
			"label": "Build",
			"type": "npm",
			"script": "build",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": [ "$tsc" ]
		},
		{
			"label": "Clear",
			"type": "npm",
			"script": "clear"
		}
	]
}
